@model IEnumerable<MySpace.Models.Artiste>

@{
    string sortDatesIconColor = ((string)Session["ArtistFieldToSort"] == "dates" ? "blue" : "gray");
    string sortUsersIconColor = ((string)Session["ArtistFieldToSort"] == "users" ? "blue" : "gray");
    string sortRatingsIconColor = ((string)Session["ArtistFieldToSort"] == "ratings" ? "blue" : "gray");
}
@foreach (var item in Model)
{
    <tr style="margin-top: 5rem">
        <td style="margin-top: 5rem">
            @Helper.AvatarProfilLink(item)
        </td>
    </tr>
}
